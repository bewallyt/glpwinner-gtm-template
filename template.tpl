// Load necessary APIs
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');
const createQueue = require('createQueue');
const log = require('logToConsole');
const makeNumber = require('makeNumber');
const queryPermission = require('queryPermission');
const callInWindow = require('callInWindow');
const getUrl = require('getUrl');

// Get tag configuration
const tagType = data.tagType;
const loadScript = data.loadScript;

// Script URL
const SCRIPT_URL = 'https://glpwinner.com/js/glp.js';

// Function to check if glp_click_id exists in URL
function hasGlpClickId() {
  const queryString = getUrl('query');
  return queryString && queryString.indexOf('glp_click_id=') !== -1;
}

// Function to build configuration object
function buildConfig() {
  const config = {
    providerId: makeNumber(data.providerId)
  };
  
  // Add optional settings
  if (data.testMode !== undefined) config.testMode = data.testMode;
  
  // Add advanced settings if enabled
  if (data.advancedSettings) {
    if (data.useSession !== undefined) config.useSession = data.useSession;
    if (data.userEmail) config.userEmail = data.userEmail;
    if (data.clearCookieOnConversion !== undefined) config.clearCookieOnConversion = data.clearCookieOnConversion;
    if (data.cookieExpiresDays) config.cookieExpiresDays = makeNumber(data.cookieExpiresDays);
    if (data.cookieNameClickId) config.cookieNameClickId = data.cookieNameClickId;
    if (data.cookieNameSessionToken) config.cookieNameSessionToken = data.cookieNameSessionToken;
  }
  
  return config;
}

// Function to initialize GLP SDK
function initializeGLP() {
  const glp = copyFromWindow('glp');
  const config = buildConfig();
  
  if (glp && glp.init) {
    log('GLP Winner: Initializing with config:', config);
    callInWindow('glp.init', config);
    data.gtmOnSuccess();
  } else {
    log('GLP Winner: SDK not ready, queueing initialization');
    // Create queue for deferred initialization
    const glpQueue = createQueue('_glp_queue');
    glpQueue({type: 'init', config: config, callback: data.gtmOnSuccess});
  }
}

// Function to build conversion data object
function buildConversionData() {
  const conversionData = {};
  
  // Add optional conversion data
  if (data.conversionEmail) conversionData.email = data.conversionEmail;
  if (data.conversionId) conversionData.conversion_id = data.conversionId;
  
  return conversionData;
}

// Function to track conversion
function trackConversion() {
  const glp = copyFromWindow('glp');
  const conversionData = buildConversionData();
  
  if (glp && glp.trackConversion) {
    log('GLP Winner: Tracking conversion with data:', conversionData);
    
    // Call trackConversion with or without data
    if (conversionData.email || conversionData.conversion_id) {
      callInWindow('glp.trackConversion', conversionData);
    } else {
      callInWindow('glp.trackConversion');
    }
    
    data.gtmOnSuccess();
  } else {
    log('GLP Winner: SDK not ready, queueing conversion');
    // Create queue for deferred conversion tracking
    const glpQueue = createQueue('_glp_queue');
    glpQueue({type: 'conversion', data: conversionData, callback: data.gtmOnSuccess});
  }
}

// Process queued commands when SDK loads
function processQueue() {
  const glpQueue = copyFromWindow('_glp_queue');
  const glp = copyFromWindow('glp');
  
  if (glpQueue && glp) {
    log('GLP Winner: Processing queued commands');
    for (let i = 0; i < glpQueue.length; i++) {
      const cmd = glpQueue[i];
      if (cmd.type === 'init' && glp.init) {
        callInWindow('glp.init', cmd.config);
        if (cmd.callback) cmd.callback();
      } else if (cmd.type === 'conversion' && glp.trackConversion) {
        // Check if data exists and has properties
        if (cmd.data && (cmd.data.email || cmd.data.conversion_id)) {
          callInWindow('glp.trackConversion', cmd.data);
        } else {
          callInWindow('glp.trackConversion');
        }
        if (cmd.callback) cmd.callback();
      }
    }
    // Clear the queue
    setInWindow('_glp_queue', [], true);
  }
}

// Main execution
if (tagType === 'init') {
  // Only initialize if glp_click_id is present in URL or forceInit is enabled
  if (!hasGlpClickId() && !data.forceInit) {
    log('GLP Winner: No glp_click_id found in URL, skipping initialization');
    data.gtmOnSuccess(); // Still call success to prevent tag errors
    return;
  }
  
  if (data.forceInit) {
    log('GLP Winner: Force initialization enabled (testing mode)');
  }
  
  if (loadScript) {
    // Load the GLP script
    if (queryPermission('inject_script', SCRIPT_URL)) {
      injectScript(SCRIPT_URL, function() {
        log('GLP Winner: Script loaded successfully');
        processQueue();
        initializeGLP();
      }, function() {
        log('GLP Winner: Failed to load script');
        data.gtmOnFailure();
      }, SCRIPT_URL);
    } else {
      log('GLP Winner: Script injection not permitted');
      data.gtmOnFailure();
    }
  } else {
    // Script should already be loaded
    initializeGLP();
  }
} else if (tagType === 'conversion') {
  // For conversions, we don't need to check for glp_click_id
  // as the SDK should already be initialized with session/cookie data
  if (loadScript) {
    // Load the GLP script
    if (queryPermission('inject_script', SCRIPT_URL)) {
      injectScript(SCRIPT_URL, function() {
        log('GLP Winner: Script loaded successfully');
        processQueue();
        trackConversion();
      }, function() {
        log('GLP Winner: Failed to load script');
        data.gtmOnFailure();
      }, SCRIPT_URL);
    } else {
      log('GLP Winner: Script injection not permitted');
      data.gtmOnFailure();
    }
  } else {
    // Script should already be loaded
    trackConversion();
  }
}


